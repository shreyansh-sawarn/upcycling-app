{
    "name": "upcycling-app-client",
    "version": "1.0.0",
    "description": "Client side of the Upcycling app",
    "author": "Shreyansh Sawarn",
    "license": "MIT",
    "private": true,
    "scripts": {
      "dev": "nuxt",
      "build": "nuxt build",
      "start": "nuxt start",
      "generate": "nuxt generate"
    },
    "dependencies": {
      "axios": "^0.21.4",
      "nuxt": "^2.15.7",
      "nuxt-fontawesome": "^0.6.1",
      "nuxt-i18n": "^7.2.1",
      "vue": "^2.6.14",
      "vuex": "^3.6.2"
    },
    "devDependencies": {
      "@nuxtjs/eslint-config": "^6.0.1",
      "babel-eslint": "^10.1.0",
      "eslint": "^7.32.0",
      "eslint-plugin-vue": "^7.17.0",
      "node-sass": "^6.0.1",
      "sass-loader": "^10.2.0"
    },
    "nuxt": {
      "buildModules": [
        "@nuxtjs/eslint-module",
        "@nuxtjs/fontawesome",
        "@nuxtjs/style-resources"
      ],
      "modules": [
        "@nuxtjs/axios",
        "nuxt-i18n"
      ],
      "styleResources": {
        "scss": [
          "~/assets/scss/_variables.scss"
        ]
      },
      "fontawesome": {
        "icons": {
          "solid": [
            "faUser",
            "faEdit",
            "faTrashAlt",
            "faStar",
            "faPlus",
            "faArrowLeft",
            "faArrowRight"
          ]
        }
      },
      "axios": {
        "baseURL": "http://localhost:3000"
      },
      "i18n": {
        "defaultLocale": "en",
        "strategy": "prefix_except_default",
        "lazy": true,
        "locales": [
          {
            "code": "en",
            "name": "English",
            "file": "en.js"
          },
          {
            "code": "fr",
            "name": "Fran√ßais",
            "file": "fr.js"
          }
        ],
        "langDir": "lang/"
      }
    }
  }
  